<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.liaofan.adminex.Mapper.AccountMapper">
    <!-- 查询用户信息 public Account getAccount(Long id);-->
    <select id="getAccount" resultType="com.liaofan.adminex.bean.Account">
        select * from account_tbl where id = #{id}
    </select>
</mapper>